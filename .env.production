# Production Environment - Frontend
# Backend API URL (через Nginx proxy)
VITE_API_BASE_URL=https://help-car.online/api/v1

# WebSocket URL (secure WebSocket через Nginx proxy)
VITE_WS_URL=wss://help-car.online

# Yandex Maps API Key
VITE_YANDEX_MAPS_API_KEY=8bfd868a-ef83-4dd0-a522-8e0bec871563

# Production режим
VITE_NODE_ENV=production

# Максимальный размер загружаемых файлов (в байтах)
VITE_MAX_FILE_SIZE=5242880

# Таймаут запросов (мс)
VITE_REQUEST_TIMEOUT=30000

# Отключить debug логи в production
VITE_DEBUG=false
