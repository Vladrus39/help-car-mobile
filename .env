# Backend API URL
VITE_API_BASE_URL=http://localhost:3000/api/v1

# WebSocket URL
VITE_WS_URL=http://localhost:3000

# Yandex Maps API Key (тот же что и в index.html)
VITE_YANDEX_MAPS_API_KEY=8bfd868a-ef83-4dd0-a522-8e0bec871563

# Режим разработки
VITE_NODE_ENV=development

# Максимальный размер загружаемых файлов (в байтах)
VITE_MAX_FILE_SIZE=5242880

# Таймаут запросов (мс)
VITE_REQUEST_TIMEOUT=30000

# Включить debug логи
VITE_DEBUG=true
